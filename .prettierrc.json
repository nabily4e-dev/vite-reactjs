{
	"semi": false,
	"singleQuote": true,
	"arrowParens": "avoid",
	"bracketSameLine": false,
	"bracketSpacing": true,
	"jsxSingleQuote": true,
	"printWidth": 80,
	"quoteProps": "as-needed",
	"tabWidth": 2,
	"trailingComma": "none",
	"useTabs": true,
	"requirePragma": false,
	"insertPragma": false,
	"proseWrap": "preserve",
	"endOfLine": "lf",
	"embeddedLanguageFormatting": "auto",
	"plugins": ["prettier-plugin-tailwindcss"]
}
