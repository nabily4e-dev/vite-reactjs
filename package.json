	"type": "module",
	"homepage": "https://github.com/YOUREPOSITORY",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/YOUREPOSITORY.git"
	},
	"author": "YOURNAME",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/YOUREPOSITORY/issues"
	},
	"engines": {
		"node": ">=18.0.0",
		"yarn": ">=1.22.0",
		"npm": "please-use-yarn"
	},
	"keywords": [
		"react",
		"vite",
		"typescript",
		"vitest",
		"tailwind",
		"eslint",
		"prettier",
		"husky",
		"commit-lint",
		"final-project",
		"template"
	],
	"scripts": {
		"dev": "vite --port 3000",
		"build": "vite build",
		"lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
		"lint:fix": "eslint . --fix --ext .js,.jsx",
		"lint:css": "stylelint \"**/*.css\"",
		"lint:css:fix": "stylelint \"**/*.css\" --fix",
		"format": "prettier . --check",
		"format:fix": "prettier . --write",
		"preview": "vite preview",
		"prepare": "husky install",
		"test": "vitest --run --reporter verbose",
		"test:w": "vitest",
		"test:u": "vitest --update --run",
		"test:ui": "vitest --ui",
		"test:c": "vitest run --coverage",
		"test:cw": "vitest watch --coverage",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"plop": "plop",
		"cz": "npx cz"
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
